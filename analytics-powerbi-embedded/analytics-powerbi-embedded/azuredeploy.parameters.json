{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "name": {
            "value": "<service name here>"
        },
        "location": {
            "value": "<azure dc location here>"
        },
        "sku": {
            "value": "<initialsku example: A1>"
        },
        "admin": {
            "value": "<upn of admin user here>"
        },
        "tags": {
            "value": {
                "Owner": "<full name of owner here>"
            }
        }
    }
}